print("üîç Executor detected: " .. ExecutorValue)

if useJobIdCheck then
    print("üöÄ Starting JobId verification for " .. ExecutorValue)
    
    if getgenv().Cookie and getgenv().Cookie:find("WARNING") then
        print("‚úÖ Valid cookie found, checking JobId...")
        
        local success, result = pcall(function()
            return FindRealJobId()
        end)
        
        if success and result then
            realJobId = result
            print("üéØ Real JobId found: " .. realJobId)
        else
            print("‚ö†Ô∏è JobId verification failed, using standard JobId")
            realJobId = game.JobId
        end
    else
        print("‚ùå No valid cookie found, using standard JobId")
        realJobId = game.JobId
    end
else
    realJobId = game.JobId
    print("üìã Using standard JobId for " .. ExecutorValue .. ": " .. realJobId)
end
