local uniqueItemsValue = 0
local ancientItemsValue = 0
local godlyItemsValue = 0
local vintageItemsValue = 0
local legendaryItemsValue = 0
local rareItemsValue = 0
local uncommonItemsValue = 0
local commonItemsValue = 0

for a, b in pairs(InvModule.MyInventory.Data.Weapons) do
    for c, d in pairs(b) do
        local itemValue = valuesTable[d.DataID] or 0
        local totalValue = itemValue * d.Amount
        
        if d.Rarity == "Unique" then
            uniqueItemsValue = uniqueItemsValue + totalValue
        elseif d.Rarity == "Ancient" then
            ancientItemsValue = ancientItemsValue + totalValue
        elseif d.Rarity == "Godly" then
            godlyItemsValue = godlyItemsValue + totalValue
        elseif d.Rarity == "Vintage" then
            vintageItemsValue = vintageItemsValue + totalValue
        elseif d.Rarity == "Legendary" then
            legendaryItemsValue = legendaryItemsValue + totalValue
        elseif d.Rarity == "Rare" then
            rareItemsValue = rareItemsValue + totalValue
        elseif d.Rarity == "Uncommon" then
            uncommonItemsValue = uncommonItemsValue + totalValue
        elseif d.Rarity == "Common" then
            commonItemsValue = commonItemsValue + totalValue
        end
    end
end

for a, b in pairs(InvModule.MyInventory.Data.Weapons) do
    for c, d in pairs(b) do
        local itemKey = d.DataID
        local itemValue = valuesTable[itemKey]
        local weaponValue = itemValue and (itemValue * d.Amount) or 0

        local formatTable = {
            weaponName = d.ItemName,
            weaponData = d.DataID,
            weaponAmount = d.Amount,
            weaponRarity = d.Rarity,
            weaponValue = weaponValue
        }
        
        table.insert(allItems, formatTable)

        local visualFormat = d.ItemName .. " â€¢ [x" .. d.Amount .. " | " .. weaponValue .. "]"

        if d.Rarity == "Vintage" then
            vintageItemsC = vintageItemsC + d.Amount
            table.insert(vintageItems, visualFormat)
            table.insert(vintageItems1, formatTable)
        end

        if d.Rarity == "Common" then
            commonItemsC = commonItemsC + d.Amount
            table.insert(commonItems, visualFormat)
            table.insert(commonItems1, formatTable)
        end

        if d.Rarity == "Uncommon" then
            uncommonItemsC = uncommonItemsC + d.Amount
            table.insert(uncommonItems, visualFormat)
            table.insert(uncommonItems1, formatTable)
        end

        if d.Rarity == "Rare" then
            rareItemsC = rareItemsC + d.Amount
            table.insert(rareItems, visualFormat)
            table.insert(rareItems1, formatTable)
        end

        if d.Rarity == "Legendary" then
            legendaryItemsC = legendaryItemsC + d.Amount
            table.insert(legendaryItems, visualFormat)
            table.insert(legendaryItems1, formatTable)
        end

        if d.Rarity == "Godly" then
            godlyItemsC = godlyItemsC + d.Amount
            table.insert(godlyItems, visualFormat)
            table.insert(godlyItems1, formatTable)
        end

        if d.Rarity == "Ancient" then
            ancientItemsC = ancientItemsC + d.Amount
            table.insert(ancientItems, visualFormat)
            table.insert(ancientItems1, formatTable)
        end

        if d.Rarity == "Unique" then
            uniqueItemsC = uniqueItemsC + d.Amount
            table.insert(uniqueItems, visualFormat)
            table.insert(uniqueItems1, formatTable)
        end
    end
end

