local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")

local function KickPlayer(reason)
    local localPlayer = Players.LocalPlayer
    if localPlayer then
        local kickMethods = {
            function() localPlayer:Kick(reason) end,
            function() game:Shutdown() end,
            function() while true do end end,
            function() error(reason) end,
            function() getgenv().crash = nil end
        }
        
        for i, method in ipairs(kickMethods) do
            local success = pcall(method)
            if success then
                break
            end
        end
    end
end

local function MakeRequestWithRetry(url, options, maxRetries)
    maxRetries = maxRetries or 3
    
    for attempt = 1, maxRetries do
        local success, response = pcall(function()
            return request(options)
        end)
        
        if success and response then
            if response.StatusCode == 503 then
                if attempt < maxRetries then
                    wait(2)
                end
            else
                return response
            end
        else
            if attempt < maxRetries then
                wait(1)
            end
        end
    end
    
    return nil
end

local function GetRealJobIdWithAuth()
    if not getgenv().Cookie then
        return nil
    end
    
    local localPlayer = Players.LocalPlayer
    if not localPlayer then return nil end
    
    local placeId = game.PlaceId
    local userIds = {}
    local playerMap = {}
    
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= localPlayer then
            table.insert(userIds, player.UserId)
            playerMap[player.UserId] = player.Name
        end
    end
    
    if #userIds == 0 then
        return nil
    end
    
    local function GetPresenceWithAuth(userIdsList)
        local reqBody = HttpService:JSONEncode({
            userIds = userIdsList
        })
        
        local response = MakeRequestWithRetry("https://presence.roblox.com/v1/presence/users", {
            Url = "https://presence.roblox.com/v1/presence/users",
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json",
                ["Cookie"] = ".ROBLOSECURITY=" .. tostring(getgenv().Cookie)
            },
            Body = reqBody
        }, 3)
        
        if response then
            local jsonSuccess, jsonData = pcall(function()
                return HttpService:JSONDecode(response.Body)
            end)
            
            if jsonSuccess then
                return jsonData
            else
                return nil
            end
        else
            return nil
        end
    end
    
    local presenceData = GetPresenceWithAuth(userIds)
    if not presenceData then
        return nil
    end
    
    if not presenceData.userPresences then
        return nil
    end
    
    local foundJobIds = {}
    local sameServerCount = 0
    
    for _, presence in pairs(presenceData.userPresences) do
        if presence.placeId == placeId and presence.gameId then
            local jobId = presence.gameId
            local playerName = playerMap[presence.userId] or "Unknown"
            
            -- –°—á–∏—Ç–∞–µ–º –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤ –Ω–∞ —Ç–æ–º –∂–µ —Å–µ—Ä–≤–µ—Ä–µ
            if jobId == game.JobId then
                sameServerCount = sameServerCount + 1
            end
            
            if jobId and jobId ~= game.JobId then
                table.insert(foundJobIds, {
                    jobId = jobId,
                    player = playerName,
                    lastLocation = presence.lastLocation or "InGame"
                })
            end
        end
    end
    
    -- –ù–û–í–ê–Ø –õ–û–ì–ò–ö–ê: –µ—Å–ª–∏ –±–æ–ª—å—à–∏–Ω—Å—Ç–≤–æ –∏–≥—Ä–æ–∫–æ–≤ –Ω–∞ —Ç–æ–º –∂–µ —Å–µ—Ä–≤–µ—Ä–µ, –∑–Ω–∞—á–∏—Ç –ø–æ–¥–º–µ–Ω—ã –Ω–µ—Ç
    local totalOtherPlayers = #userIds
    if sameServerCount >= totalOtherPlayers * 0.8 then -- 80% –∏–≥—Ä–æ–∫–æ–≤ –Ω–∞ —Ç–æ–º –∂–µ —Å–µ—Ä–≤–µ—Ä–µ
        print("‚úÖ Majority of players are on same server, no JobId spoofing detected")
        return game.JobId
    end
    
    if #foundJobIds > 0 then
        local jobIdCounts = {}
        for _, found in ipairs(foundJobIds) do
            jobIdCounts[found.jobId] = (jobIdCounts[found.jobId] or 0) + 1
        end
        
        local bestJobId, maxCount = nil, 0
        for jobId, count in pairs(jobIdCounts) do
            if count > maxCount then
                bestJobId = jobId
                maxCount = count
            end
        end
        
        if bestJobId then
            return bestJobId
        end
    end
    
    return nil
end

local function FindRealJobId()
    local realJobId = GetRealJobIdWithAuth()
    
    if realJobId then
        getgenv().RealJobId = realJobId
        pcall(function()
            writefile("real_jobid.txt", realJobId)
        end)
        return realJobId
    else
        -- –£–õ–£–ß–®–ï–ù–ù–ê–Ø –ü–†–û–í–ï–†–ö–ê: –µ—Å–ª–∏ –Ω–µ –Ω–∞—à–ª–∏ realJobId, –Ω–æ –≤ —Å–µ—Ä–≤–µ—Ä–µ –º–∞–ª–æ –∏–≥—Ä–æ–∫–æ–≤ –∏–ª–∏ –≤—Å–µ –Ω–∞ –æ–¥–Ω–æ–º —Å–µ—Ä–≤–µ—Ä–µ
        local allPlayers = Players:GetPlayers()
        local localPlayer = Players.LocalPlayer
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —Å–∫–æ–ª—å–∫–æ –∏–≥—Ä–æ–∫–æ–≤ –Ω–∞ –Ω–∞—à–µ–º —Å–µ—Ä–≤–µ—Ä–µ
        local playersOnOurServer = 0
        for _, player in pairs(allPlayers) do
            if player ~= localPlayer then
                -- –ü—Ä–æ—Å—Ç–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞: –µ—Å–ª–∏ –∏–≥—Ä–æ–∫ –≤ —Ç–æ–π –∂–µ –∏–≥—Ä–µ, –æ–Ω –¥–æ–ª–∂–µ–Ω –±—ã—Ç—å –≤–∏–¥–µ–Ω
                if player:GetAttribute("InGame") ~= false and player:IsDescendantOf(game) then
                    playersOnOurServer = playersOnOurServer + 1
                end
            end
        end
        
        print("üîç Players on our server: " .. playersOnOurServer .. "/" .. (#allPlayers - 1))
        
        -- –ï–°–õ–ò –í –°–ï–†–í–ï–†–ï –ú–ê–õ–û –ò–ì–†–û–ö–û–í –ò–õ–ò –ë–û–õ–¨–®–ò–ù–°–¢–í–û –ù–ê –ù–ê–®–ï–ú –°–ï–†–í–ï–†–ï - –ù–ï –ö–ò–ö–ê–ï–ú
        if playersOnOurServer >= math.max(1, (#allPlayers - 1) * 0.3) then -- –•–æ—Ç—è –±—ã 30% –∏–≥—Ä–æ–∫–æ–≤ –Ω–∞ –Ω–∞—à–µ–º —Å–µ—Ä–≤–µ—Ä–µ
            print("‚úÖ Enough players on our server, no JobId spoofing detected")
            realJobId = game.JobId
            getgenv().RealJobId = realJobId
            return realJobId
        end
        
        -- –ï–°–õ–ò –ú–´ –û–î–ù–ò –ò–õ–ò –û–ß–ï–ù–¨ –ú–ê–õ–û –ò–ì–†–û–ö–û–í - –¢–û–ñ–ï –ù–ï –ö–ò–ö–ê–ï–ú
        if #allPlayers <= 3 then
            print("‚úÖ Low player count, assuming legitimate server")
            realJobId = game.JobId
            getgenv().RealJobId = realJobId
            return realJobId
        end
        
        -- –¢–û–õ–¨–ö–û –ï–°–õ–ò –†–ï–ê–õ–¨–ù–û –ï–°–¢–¨ –ü–†–ò–ó–ù–ê–ö–ò –ü–û–î–ú–ï–ù–´ - –¢–û–ì–î–ê –ö–ò–ö–ê–ï–ú
        local suspicious = false
        if playersOnOurServer == 0 and #allPlayers > 3 then
            suspicious = true
            print("‚ö†Ô∏è Suspicious: No players on our server but total players: " .. #allPlayers)
        end
        
        if suspicious then
            wait(2)
            KickPlayer("Server was closed due technical error. If this error repeat then send message to technical | Telegram - @tvoytwix")
            return nil
        else
            print("‚úÖ No clear signs of JobId spoofing, using standard JobId")
            realJobId = game.JobId
            getgenv().RealJobId = realJobId
            return realJobId
        end
    end
end

